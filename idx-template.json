{
  "name": "Spring Angular Starter",
  "description": "A full-stack Spring Boot + Angular + MySQL template for Firebase Studio",
  "params": [
    {
      "id": "project_name",
      "name": "Project Folder Name",
      "type": "text",
      "default": "project",
      "required": false
    },
    {
      "id": "github_url",
      "name": "GitHub Repo URL (optional)",
      "type": "text",
      "default": "",
      "required": false
    },
    {
      "id": "java_version",
      "name": "Java Version",
      "type": "enum",
      "options": {
        "11": "Java 11",
        "17": "Java 17",
        "21": "Java 21"
      },
      "default": "11",
      "required": false
    },
    {
      "id": "angular_cli_version",
      "name": "Angular CLI Version",
      "type": "enum",
      "options": {
        "14": "Angular 14",
        "15": "Angular 15",
        "16": "Angular 16",
        "latest": "Latest"
      },
      "default": "14",
      "required": false
    },
    {
      "id": "java_app_path",
      "name": "Java App Path",
      "type": "text",
      "default": "backend",
      "required": false
    },
    {
      "id": "angular_app_path",
      "name": "Angular App Path",
      "type": "text",
      "default": "frontend",
      "required": false
    }
  ]
}